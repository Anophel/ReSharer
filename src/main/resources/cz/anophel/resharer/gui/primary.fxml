<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="204.0" prefWidth="356.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.anophel.resharer.gui.PrimaryController">
   <children>
      <Label text="Welcome to ReSharer">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label text="Use as" />
      <Button fx:id="clientButton" onAction="#switchToUser" text="Client" />
      <Button fx:id="serverButton" mnemonicParsing="false" onAction="#switchToServer" text="Server" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
